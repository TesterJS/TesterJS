{
  "name": "javascript-performance-tester",
  "version": "0.1.0",
  "description": "JavaScript Perfomance Tester tool",
  "main": "index.js",
  "engines": {
    "node": "6.11.0"
  },
  "config": {
    "blanket": {
      "pattern": "./test/**/*-test.js",
      "data-cover-never": [
        "node_modules",
        "public"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  },
  "scripts": {
    "start": "node index.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siciliajuan/Tester.git"
  },
  "keywords": [
    "js",
    "javascript",
    "tester",
    "performance"
  ],
  "author": "Juan Sicilia, JuanMa Ruiz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/siciliajuan/Tester/issues"
  },
  "homepage": "https://github.com/siciliajuan/Tester#readme",
  "devDependencies": {
    "chai": "4.1.1",
    "istanbul": "0.4.5",
    "mocha": "3.5.0"
  }
}
